  <TemplateContent>
    <Project TargetFileName="$ext_safeprojectname$.Domain.csproj" File="AppName.Domain.csproj" ReplaceParameters="true">
      <Folder Name="Entities" TargetFolderName="Entities">
        <ProjectItem ReplaceParameters="true" TargetFileName="Customer.cs">Customer.cs</ProjectItem>
      </Folder>
    </Project>
  </TemplateContent>
</VSTemplate>